{
  "name": "vanilla-web-components",
  "version": "0.0.1",
  "description": "Vanilla css web components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/vanilla-web-components/vanilla-web-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build:static": "stencil build --prerender && ./prefix-docs-links.sh",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "dependencies": {
    "@stencil/core": "^3.0.0",
    "classnames": "^2.3.2",
    "vanilla-framework": "^3.12.1"
  },
  "devDependencies": {
    "@stencil/react-output-target": "^0.5.0",
    "@stencil/sass": "^3.0.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "^19.5.2"
  },
  "license": "MIT"
}
